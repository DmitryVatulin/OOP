#include <iostream>
#include <string>
#include <Windows.h>
#include <stdlib.h>
using namespace std;

class Plumber {
private:
	string name;
	string surname;
	string experience;
	int age;
public:Plumber() {}
	   int a;
	   Plumber(string name, string surname, string experience, int age) {
		   this->age = age;
		   this->name = name;
		   this->surname = surname;
		   this->experience = experience;
	   };

	   void print_value() {
		   cout << "Имя: " << name << endl;
		   cout << "Фамилия: " << surname << endl;
		   cout << "Стаж работы: " << experience << endl;
		   cout << "Возраст: " << age << endl;
	   };

};
